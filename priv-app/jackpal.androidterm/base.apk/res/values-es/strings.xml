    <string name="control_key_dialog_control_disabled_text">No hay tecla definida para Ctrl.</string>
    <string name="control_key_dialog_control_text">"CTRLKEY Espacio : Control-@ (NUL)
CTRLKEY A..Z : Control-A..Z
CTRLKEY 5 : Control-]
CTRLKEY 6 : Control-^
CTRLKEY 7 : Control-_
CTRLKEY 9 : F11
CTRLKEY 0 : F12"</string>
    <string name="control_key_dialog_fn_disabled_text">No hay tecla definida para FN.</string>
    <string name="control_key_dialog_fn_text">"FNKEY 1..9 : F1-F9
FNKEY 0 : F10
FNKEY W : Up
FNKEY A : Left
FNKEY S : Down
FNKEY D : Right
FNKEY P : AvPágina
FNKEY N : RetPágina
FNKEY T : Tab
FNKEY L : | (tubería)
FNKEY U : _ (subrayado)
FNKEY E : Control-[ (ESC)
FNKEY X : Suprimir
FNKEY I : Insertar
FNKEY H : Inicio
FNKEY F : Fin
FNKEY . : Control-\\
"</string>
